{
  "name": "ast-parse",
  "version": "0.1.0",
  "description": "Tools to help facilitate AST parsing of TS/JS content",
  "license": "MIT",
  "author": "Ken Snyder <ken@ken.net>",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/typings/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist/**",
    "build": "pnpm run clean && npx node devops/build commonjs esnext --min",
    "build:analyze": "npx node devops/build esnext --analyze --min",
    "lint": "run-s lint:src lint:test lint:tsc",
    "lint:src": "npx eslint src/**/*.ts --fix",
    "lint:test": "npx eslint test/**/*.ts --fix",
    "lint:tsc": "npx tsc --noEmit",
    "watch": "ttsc -P tsconfig.build.json -w"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "brilliant-errors": "^0.3.0",
    "common-types": "^1.25.0",
    "dd": "^0.9.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "native-dash": "^1.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "recast": "^0.20.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  },
  "peerDependencies": {
    "brilliant-errors": "^0.3.0",
    "recast": "^0.20.4"
  }
}
